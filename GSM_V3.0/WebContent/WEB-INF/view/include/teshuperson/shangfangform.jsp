<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
    <form class="form-horizontal" role="form" id="shangFangForm">
    <div class="form-group">
	<label class="col-xs-2 control-label no-padding-right text-right" >开始上访时间</label>
	<div class="col-xs-4">
		<input type="text" max-length="50"name="petitionDate" value="${vdata.house.petitionDate }" class="col-xs-12 col-sm-12 tooltip-error" />
	</div>
	<label class="col-xs-2 control-label no-padding-right text-right" >问题管辖部门</label>
	<div class="col-xs-4">
		<input type="text" max-length="50"name="problemScope" value="${vdata.house.problemScope }" class="col-xs-12 col-sm-12 tooltip-error" />
	</div>
</div>
<div class="form-group">
	<label class="col-xs-2 control-label no-padding-right text-right" >上访事由</label>
	<div class="col-xs-4">
		<input type="text" max-length="50"name="petitionReason" value="${vdata.house.petitionReason }" class="col-xs-12 col-sm-12 tooltip-error" />
	</div>
	<label class="col-xs-2 control-label no-padding-right text-right" >处理结论</label>
	<div class="col-xs-4">
		<input type="text" max-length="50"name="handleConclusion" value="${vdata.house.handleConclusion }" class="col-xs-12 col-sm-12 tooltip-error" />
	</div>
</div>
<div class="form-group">
	<label class="col-xs-2 control-label no-padding-right text-right" >受处理情况</label>
	<div class="col-xs-4">
		<input type="text" max-length="50"name="handleSituation" value="${vdata.house.handleSituation }" class="col-xs-12 col-sm-12 tooltip-error" />
	</div>
	<label class="col-xs-2 control-label no-padding-right text-right" >稳控措施</label>
	<div class="col-xs-4">
		<input type="text" max-length="50"name="controlMeasures" value="${vdata.house.controlMeasures }" class="col-xs-12 col-sm-12 tooltip-error" />
	</div>
</div>
<div class="form-group">
	<label class="col-xs-2 control-label no-padding-right text-right" >管控人员</label>
	<div class="col-xs-4">
		<input type="text" max-length="50"name="controlPerson" value="${vdata.house.controlPerson }" class="col-xs-12 col-sm-12 tooltip-error" />
	</div>
	<label class="col-xs-2 control-label no-padding-right text-right" >管控人员电话</label>
	<div class="col-xs-4">
		<input type="text" max-length="50"name="controlPhone" value="${vdata.house.controlPhone }" class="col-xs-12 col-sm-12 tooltip-error" />
	</div>
</div>
	</form>
