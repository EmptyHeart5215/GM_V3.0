package com.dykj.GSM.model.zongzhitraining;

import javax.persistence.Transient;

import com.dykj.GSM.annotation.MapperUrlAnnotion;
import com.dykj.GSM.model.BaseModel;

/**
 * 综治材料管理
 * 
 * @author wangmingyuan
 */
@MapperUrlAnnotion(mapperUrl = "com.dykj.GSM.mapper.zongzhitraining.mapperinterf.ZongzhiMaterialMapper")
public class ZongzhiMaterial extends BaseModel {

	/**
	 * 所属辖区或部门
	 */
	private String girdCode;
	/**
	 * 资料名称
	 */
	private String materialName;
	/**
	 * 资料大小;
	 */
	private String materialSize;
	/**
	 * 资料类型
	 */
	private String codeMaterialType;
	/**
	 * 上传学习资料类型
	 */
	private String codeUploadMaterialType;
	/**
	 * 学习资料内容
	 */
	private String materialContent;
	/**
	 * 资料下载次数
	 */
	private String downloadCount;
	/**
	 * 下载时间或阅读时间
	 */
	private String downLoadTime;
	/**
	 * 阅读或下载人员
	 */
	private String downLoadUserCode;

	/**
	 * 文件code
	 */
	private String materialFile;

	/**
	 * 创建人员姓名
	 */
	@Transient
	private String createUserName;

	// 文件上传时间
	// 文件上传人
	// 均在BaseModel里面封装了

	public String getGirdCode() {
		return girdCode;
	}

	public void setGirdCode(String girdCode) {
		this.girdCode = girdCode;
	}

	public String getMaterialName() {
		return materialName;
	}

	public void setMaterialName(String materialName) {
		this.materialName = materialName;
	}

	public String getMaterialSize() {
		return materialSize;
	}

	public void setMaterialSize(String materialSize) {
		this.materialSize = materialSize;
	}

	public String getCodeMaterialType() {
		return codeMaterialType;
	}

	public void setCodeMaterialType(String codeMaterialType) {
		this.codeMaterialType = codeMaterialType;
	}

	public String getCodeUploadMaterialType() {
		return codeUploadMaterialType;
	}

	public void setCodeUploadMaterialType(String codeUploadMaterialType) {
		this.codeUploadMaterialType = codeUploadMaterialType;
	}

	public String getMaterialContent() {
		return materialContent;
	}

	public void setMaterialContent(String materialContent) {
		this.materialContent = materialContent;
	}

	public String getDownloadCount() {
		return downloadCount;
	}

	public void setDownloadCount(String downloadCount) {
		this.downloadCount = downloadCount;
	}

	public String getDownLoadTime() {
		return downLoadTime;
	}

	public void setDownLoadTime(String downLoadTime) {
		this.downLoadTime = downLoadTime;
	}

	public String getDownLoadUserCode() {
		return downLoadUserCode;
	}

	public void setDownLoadUserCode(String downLoadUserCode) {
		this.downLoadUserCode = downLoadUserCode;
	}

	public String getMaterialFile() {
		return materialFile;
	}

	public void setMaterialFile(String materialFile) {
		this.materialFile = materialFile;
	}

	public String getCreateUserName() {
		return createUserName;
	}

	public void setCreateUserName(String createUserName) {
		this.createUserName = createUserName;
	}

}
