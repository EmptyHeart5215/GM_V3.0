<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<jsp:include  page="/resource/base_import.jsp"/>
</head>
<body>

<div id="print"style="width:100%;height:400px;padding:20px;">
					<h2><center>单位门店概况</center></h2>
						<table class="print-table" >
							<tr>
								<td class="col-xs-3">单位门店名称</td><td class="print-normal col-xs-3">${store.name}</td>
								<td class="col-xs-3">经营范围</td><td class="print-normal col-xs-3">${store.authority}</td>
							</tr>
							<tr>
								<td class="col-xs-3">地址</td><td colspan="3" class="print-normal col-xs-9">${store.address}</td>
							</tr>
							<tr>
								<td class="col-xs-3">业务主管部门</td><td class="print-normal col-xs-3">${store.businessSector}</td>
								<td class="col-xs-3">从属部门</td><td class="print-normal col-xs-3">${store.departmentCode}</td>
							</tr>
							<tr>
								<td class="col-xs-3">证件编号</td><td class="print-normal col-xs-3">${store.documentNum}</td>
								<td class="col-xs-3">房屋来源</td><td class="print-normal col-xs-3">${store.dicValueMap.codeHouseSource}</td>
							</tr>
							<tr>
								<td class="col-xs-3">有无安保设施</td><td class="print-normal col-xs-3">${store.idSecurity}</td>
								<td class="col-xs-3">有无团组织</td><td class="print-normal col-xs-3">${store.isManagement}</td>
							</tr>
							<tr>
								<td class="col-xs-3">有无党组织</td><td class="print-normal col-xs-3">${store.isParty}</td>
								<td class="col-xs-3">有无综治组织</td><td class="print-normal col-xs-3">${store.isUnion}</td>
							</tr>
							<tr>
								<td class="col-xs-3">修改人</td><td class="print-normal col-xs-3">${store.lastModifiedBy}</td>
								<td class="col-xs-3">法人代表</td><td class="print-normal col-xs-3">${store.legalCode}</td>
							</tr>
							<tr>
								<td class="col-xs-3">法人代表电话</td><td class="print-normal col-xs-3">${store.legalPhone}</td>
								<td class="col-xs-3">团组织编号</td><td class="print-normal col-xs-3">${store.managementCode}</td>
							</tr>
							<tr>
								<td class="col-xs-3">团组织负责人</td><td class="print-normal col-xs-3">${store.managementDuty}</td>
								<td class="col-xs-3">工会编号</td><td class="print-normal col-xs-3">${store.organationCode}</td>
							</tr>
							<tr>
								<td class="col-xs-3">工会织负责人</td><td class="print-normal col-xs-3">${store.organationDuty}</td>
								<td class="col-xs-3">在职人数</td><td class="print-normal col-xs-3">${store.peopleWorkNum}</td>
							</tr>
							<tr>
								<td class="col-xs-3">登记注册时间</td><td class="print-normal col-xs-3">${store.registerDate}</td>
								<td class="col-xs-3">类型</td><td class="print-normal col-xs-3">${store.type}</td>
							</tr>
							<tr>
								<td class="col-xs-3">综治组织编号</td><td class="print-normal col-xs-3">${store.unionCode}</td>
								<td class="col-xs-3">综治负责人</td><td class="print-normal col-xs-3">${store.unionDuty}</td>
							</tr>
							<tr>
								<td class="col-xs-3">开始使用年份</td><td class="print-normal col-xs-3">${store.useStartDate}</td>
								<td class="col-xs-3">网格联系人</td><td class="print-normal col-xs-3">${store.gridName}</td>
							</tr>
								<tr>
								<td class="col-xs-3">网格联系人职务</td><td class="print-normal col-xs-3">${store.gridDuty}</td>
								<td class="col-xs-3">网格联系人电话</td><td class="print-normal col-xs-3">${store.gridPhone}</td>
							</tr>
						</table>
					</div>
</body>

</html>